<h1>Eat Da Burger!</h1>

<h2>Burgers not devoured</h2>

<ul>
    {{#each burger}}
      {{#unless this.devoured}}
        <li>{{this.id}}.{{this.burger_name}}</li>
      {{/unless}}
    {{/each}}
</ul>

<h2>Burgers devoured</h2>

<ul>
    {{#each burgers}}
      {{#if this.devoured}}
        <li>{{this.id}}.{{this.burger_name}}</li>
      {{/if}}
    {{/each}}
</ul>